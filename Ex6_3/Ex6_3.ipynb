    "def derivative(f):\n",
    "    dfdx = []\n",
    "    for i, coeff in enumerate(f):\n",
    "        if i == 0:\n",
    "            continue\n",
    "        dfdx.append(i * coeff)\n",
    "    return dfdx\n",
    "\n",
    "f = [4,5,0,2]\n",
    "print(derivative(f))\n",
    "\n",
    "f = [10,1,1,1,1,1]\n",
    "print(derivative(f))"
   ]
  }
